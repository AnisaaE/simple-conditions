using System;
					
public class Program
{
	public static void Main()
	{
		int holidays= int.Parse(Console.ReadLine()); 
		int workDays = 365-holidays;
		var playM =  holidays*127 + workDays *63;
		var difference= Math.Abs(30000- playM);
		var hours = difference/60;
		var min = difference%60;
		if (playM<=30000)
		{
		 Console.WriteLine("Tom sleeps well.");
		 Console.WriteLine("{0} hours and {1} minutes less for play.", hours, min);
		
		}
		else
		{Console.WriteLine ("Tom will run away");
		Console.WriteLine("{0} hours and {1} minutes more to play", hours, min);
		}
		
		
	}
}
